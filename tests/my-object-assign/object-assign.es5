var obj = {
  a: 1,
  b: true,
  c: function () {},
  d: null,
  e: 'e'
};

var fn = function (src) {
  var o = {};
  for (var i in src) {
    o[i] = src[i];
  }
  return o;
};

var r = fn(obj);
assertEqual(r.a, obj.a);
assertEqual(r.b, obj.b);
assertEqual(r.c, obj.c);
assertEqual(r.d, obj.d);
assertEqual(r.e, obj.e);

test(function () {
  fn(obj);
});
