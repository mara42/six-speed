var data = [2,2*10, 2*4, 2*90, 2*2001]

function isEven(a) {
    // using modulo to handle floats better
    return a % 2 === 0
}

function fn() {
  for (var i = 0; i < data.length; i++) {
    if (!isEven(data[i])) {
        return false
    }
  }
  return true
}

assertEqual(fn(), true);

test(fn);
