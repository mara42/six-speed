var data = []
for (var i=0; i < 4e4; i++) {
  data.push(2*i)
}

function isEven(a) {
    // using modulo to handle floats better
    return a % 2 === 0
}

function fn() {
  for (var i = 0; i < data.length; i++) {
    if (!isEven(data[i])) {
        return false
    }
  }
  return true
}

assertEqual(fn(), true);

test(fn);
